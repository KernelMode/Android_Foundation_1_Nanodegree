<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ankur.musicstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Music"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
		  <meta-data
        android:name="com.google.android.gms.version"
        android:value="@integer/google_play_services_version" />
		   <meta-data
        android:name="com.google.android.gms.wallet.api.enabled"
        android:value="true" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NowPlaying" />
        <activity android:name=".SongActivity" />
        <activity android:name=".AlbumActivity" />
        <activity android:name=".SpotifyActivity"></activity>
		<activity android:name=".PaymentActivity"></activity>
    </application>

</manifest>